name: Suspend SSAS Dev

on:
  workflow_dispatch:  
  schedule:
  - cron: "0 18 * * *"

jobs:
  Deploy:
    name: Release to test
    uses: ./.github/workflows/master_release_services_dev.yml
    with:
      DeploySSMS: false
      DeployADF: false
      DeploySSAS: false
      DeploySSASModel: "Template"
      SuspendSSAS: true
    secrets: inherit
