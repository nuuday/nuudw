{
    "name": "LoadDimension_<%ADFTableName%>",
    "properties": {
        "activities": [
            {
                "name": "SP_LoadDimension<%ADFTableName%>",
                "type": "SqlServerStoredProcedure",
                "policy": {
                    "timeout": "0.12:00:00",
                    "retry": 0,
                    "retryIntervalInSeconds": 30,
                    "secureOutput": false,
                    "secureInput": false
                },
                "typeProperties": {
                    "storedProcedureName": "[meta].[LoadDimension]",
		    "storedProcedureParameters": {
                        "Table": {
                            "value": "<%ADFTableName%>",
                            "type": "String"
                        	},
			"LoadIsIncremental": {
                            "value": "false",
                            "type": "Boolean"
                        	},
			"DisableMaintainDWFlag": {
                            "value": "@pipeline().parameters.DisableMaintainDW",
                            "type": "Boolean"
                        	},
			"PrintSQL": {
                            "value": "false",
                            "type": "Boolean"
                        	}
                	}
                },
                "linkedServiceName": {
                    "referenceName": "<%ADFLinkedServiceName%>",
                    "type": "LinkedServiceReference"
                }
            }
        ],
	"parameters": {        
	"DisableMaintainDW": {
          "type": "Bool",
          "defaultValue": "false",
          "identity": "DisableMaintainDW"
        	}
	},      
	  "folder": {
            "name": "2.Dimensions"
        }
    }    
}
