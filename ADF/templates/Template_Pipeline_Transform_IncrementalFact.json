                        },
                        "StoredProcedureName": {
                          "value": "Transform_<%ADFTableName%>",
                          "type": "String"
                        },
                        "DestinationSchema": {
                          "value": "<%ADFSchemaName%>",
                          "type": "String"
                    }
                  }
		                      },
                "linkedServiceName": {
                    "referenceName": "<%ADFLinkedServiceName%>",
                    "type": "LinkedServiceReference"                
	     	}
            }
        ],
      "parameters": {        
        "JobIsIncremental": {
          "type": "Bool",
          "defaultValue": "false",
          "identity": "JobIsIncremental"
        }
	},
	  "folder": {
            "name": "5.Transform"
        }
    }    
